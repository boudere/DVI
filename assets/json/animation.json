{
    "Animation": {
        "fade_in": {
            "alpha_start": 0,
            "alpha_end": 1
        },
        "fade_out": {
            "alpha_start": 1,
            "alpha_end": 0
        },
        "move_left": {
            "pos_x_end": "$pos_x_start {sub} [pos_x]"
        },
        "move_right": {
            "pos_x_end": "$pos_x_start {add} [pos_x]"
        },
        "move_up": {
            "pos_y_end": "$pos_y_start {sub} [pos_y]"
        },
        "move_down": {
            "pos_y_end": "$pos_y_start {add} [pos_y]"
        },
        "scale_center": {
            "scaleX_start": 0,
            "scaleY_start": 0,
            "scaleX_end": "[scale]",
            "scaleY_end": "[scale]"
        },
        "scale_up": {
            "scaleY_start": 0,
            "scaleX_start": "[scaleX]",
            "scaleY_end": "[scaleY]"
        }
    }
}